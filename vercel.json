{
  "redirects": [
    {
      "source": "/",
      "destination": "/auth/login",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|.*\\..*).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/((?!.*\\.).*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    }
  ]
}
